{
  "apiVersion": "apps/v1",
  "kind": "Deployment",
  "metadata": {
    "annotations": {},
    "name": "vote-front",
    "namespace": "default"
  },
  "spec": {
    "minReadySeconds": 5,
    "replicas": 1,
    "selector": {
      "matchLabels": {
        "app": "vote-front"
      }
    },
    "strategy": {
      "rollingUpdate": {
        "maxSurge": 1,
        "maxUnavailable": 1
      }
    },
    "template": {
      "metadata": {
        "labels": {
          "app": "vote-front"
        }
      },
      "spec": {
        "containers": [
          {
            "env": [
              {
                "name": "REDIS",
                "value": "vote-back"
              }
            ],
            "image": "mcr.microsoft.com/azuredocs/azure-vote-front:v1",
            "name": "vote-front",
            "ports": [
              {
                "containerPort": 80
              }
            ],
            "resources": {
              "limits": {
                "cpu": "500m"
              },
              "requests": {
                "cpu": "250m"
              }
            }
          }
        ]
      }
    }
  }
}