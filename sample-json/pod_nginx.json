{
  "apiVersion": "v1",
  "kind": "Pod",
  "metadata": {
    "annotations": {},
    "labels": {
      "app": "web"
    },
    "name": "nginx",
    "namespace": "default"
  },
  "spec": {
    "containers": [
      {
        "image": "nginx",
        "name": "nginx",
        "ports": [
          {
            "containerPort": 80
          }
        ]
      }
    ]
  }
}